<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.danirg10000gmail.HelpFromAfar"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name="com.danirg10000gmail.HelpFromAfar.TherapistApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.QuestionViewPagerActivity"
            android:label="@string/Questionnaire">
        </activity>
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.QuestionnaireListActivity"
            android:label="@string/Questionnaires"
            android:theme="@style/AppTheme"/>
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.registration.CreateAccountActivity"
            android:label="@string/reg"/>
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.registration.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.user.UsersListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.userData.EditProfileActivity"
            android:label="@string/edit">
        </activity>
        <activity
            android:name="com.danirg10000gmail.HelpFromAfar.userData.ProfileActivity"
            android:label="@string/profile">
        </activity>
    </application>

</manifest>
